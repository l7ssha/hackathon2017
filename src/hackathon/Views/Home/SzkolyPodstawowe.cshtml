@model IEnumerable<SzkolaPodstawowa>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col"Szkola</th>
      <th scope="col">Adres</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
      @foreach(var item in Model)
      {
        <tr>
        <th scope="row">@item.Id</th>
        <td>@item.Name</td>
        <td>@item.Address</td>
        <td>@item.Email</td>
        </tr>
    }
  </tbody>
</table>